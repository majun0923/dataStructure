
class Solution {
    public int search(int[] nums, int target) {
        int res = -1,s=0,e=nums.length-1;
        while(s<=e){//æ³¨æ„
            int mid = s+(e-s)/2;
            if(nums[mid]==target){
                res = mid;
                return res;
            }else if(nums[mid]<target){
                s=mid+1;
            } else if (nums[mid] > target){
                e=mid-1;
            }
        }
        return res;
    }
}